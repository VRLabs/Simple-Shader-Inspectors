{
  "metadata": [],
  "build": {
    "content": [],
    "resource": [
      {
        "files": [
          "images/**",
          "logo.png",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Simple Shader Inspectors Documentation",
      "_appFooter": "<span>Copyright Â© VRLabs.<br>Generated by <strong>DocFX</strong></span>",
      "_disableContribution": true,
      "_enableSearch": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx",
      "templates/memberpage/content"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "sitemap": {
      "baseUrl": "https://ssi.vrlabs.dev",
      "priority": 0.4,
      "changefreq": "monthly"
    }
  }
}